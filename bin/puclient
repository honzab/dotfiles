#!/usr/bin/env python
import sys
import xmlrpclib
import code

server = sys.argv[1] if len(sys.argv) == 2 else 'localhost'
pu = xmlrpclib.ServerProxy("http://%s:5101/RPC2" % server)
api = xmlrpclib.ServerProxy("http://%s:5180/RPC2" % server)

print "product-user:", pu
print "api:", api
code.interact("", local={'pu': pu, 'api': api})
